(re-seq #":" "lol:bar")
(def text ":")
(re-seq #text "lol:bar")
(re-seq #"(text)" "lol:bar")
(re-seq #(text) "lol:bar")
(def is-word? (set ["julien" "lol"]))
(not (nil? (some is-word? ("@julien hey"))))
(not (nil? (some is-word? (.split " " "@julien hey"))))
(not (nil? (some is-word? (.split " " "julien hey"))))
(not (nil? (some is-word? (.split "@julien hey" " "))))
(not (nil? (some is-word? (.split "julien hey" " "))))
(def regex (re-pattern (apply str text)))
(re-seq regex "lo:bar")
